{
  "name": "injectinator",
  "version": "1.1.0",
  "description": "An easy-to-use, powerful dependency injection library for TypeScript",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "jest --coverage",
    "test:watch": "yarn test --watch",
    "lint": "eslint src/",
    "lint:fix": "yarn lint --fix",
    "clean": "rimraf dist/ *.tsbuildinfo",
    "watch": "tsc --watch --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DonIsaac/Injectionator.git"
  },
  "keywords": [
    "dependency",
    "injection",
    "typescript",
    "node",
    "di"
  ],
  "author": "Donald Isaac",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DonIsaac/Injectionator/issues"
  },
  "homepage": "https://github.com/DonIsaac/Injectionator#readme",
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "tslib": "^1.11.0"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.6",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.19",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
